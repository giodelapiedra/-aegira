    "@supabase/supabase-js": "^2.89.0",
    "busboy": "^1.6.0",
    "dotenv": "^17.2.3",
    "hono": "^4.11.3",
    "jose": "^6.1.3",
    "luxon": "^3.7.2",
    "node-cron": "^4.2.1",
    "openai": "^6.15.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^2.4.5",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "prisma": "^6.19.1",
