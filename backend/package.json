    "@types/node": "^25.0.3",
    "@types/node-cron": "^3.0.11",
    "@types/pdf-parse": "^1.1.5",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^4.0.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
